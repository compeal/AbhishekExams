<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.zorba.entity.Product_Type" table="account">
        <id name="product_type_id" column="product_type_id">
            <generator class="increment"/>
        </id>
        <property name="productType" column="product_type" length="255"/>

        <property name="productRate" column="product_rate"  length="10"/>
    </class>
</hibernate-mapping>
